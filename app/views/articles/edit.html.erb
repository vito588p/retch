<h1>編輯文章</h1>
<div>
    <!-- Form Helpers -->
    <%= form_with(model: @article, data: { turbo: false}) do |f| %>
        <div>
            <%= f.label :title, "標題" %>
            <%= f.text_field :title %>
        </div>
        <div>
            <%= f.label :content, "內文" %>
            <%= f.text_area :content %>
        </div>

        <%= f.submit %>
    <% end %>
</div>